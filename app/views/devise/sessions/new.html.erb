<div class="row">
    <div class="col-md-4">
        <img src="/images/bhealth.png">
    </div>
<div class="col-md-4">
<div class="form-signin">
<div class="box-login">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

  <center><h2 class="color-like">Ingresar</h2></center>
  <div class="field">
    <%= f.label :email,class:"color-like" %><br />
    <%= f.email_field :email, class:"form-control", autofocus: true %>
  </div>

  <div class="field">
    <%= f.label :password,class:"color-like" %><br />
    <%= f.password_field :password, class:"form-control", autocomplete: "off" %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me,class:"color-like" %>
    </div>
  <% end -%>

  <div class="actions">
    <%= f.submit "Log in",class:"btn btn-success" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>
</div>
</div>
    <div class="col-md-4">
        <img src="/images/bhealth.png">
    </div>
</div>
